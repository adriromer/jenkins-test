pipeline {
       agent any
        options {
            skipDefaultCheckout()
            disableConcurrentBuilds()
            timeout(5)
            buildDiscarder(logRotator(numToKeepStr: '5'))
            ansiColor('xterm')  
        }
       agent { label "maven" } 
    stages {
        stage('Git Checkout') {
            steps {
            gitCheckout(
                branch: "master",
                url: "https://github.com/spring-projects/spring-petclinic.git"
            )
            }
        }
    }
}


