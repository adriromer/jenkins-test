
pipeline { 
    agent any  
    stages { 
        stage('CheckOut') { 
            steps { 
               checkout([
               $class: 'GitSCM', 
               branches: [[name: '*/master']], 
               doGenerateSubmoduleConfigurations: false, 
               extensions: [], 
               submoduleCfg: [], 
               userRemoteConfigs: [[url: 'https://github.com/spring-projects/spring-petclinic.git']]
               ]) 
               echo 'This is a minimal pipeline.' 
            }
        }
    }
}



